{
    "name": "riverskies/laravel-newsletter-subscription",
    "description": "Simple newsletter subscription for Laravel projects.",
    "license": "MIT",
    "authors": [
        {
            "name": "Barnabas Kecskes",
            "email": "barnabas@riverskies.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "hashids/hashids": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3",
        "orchestra/testbench": "^3.5",
        "vlucas/phpdotenv": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Riverskies\\LaravelNewsletterSubscription\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Riverskies\\LaravelNewsletterSubscription\\Providers\\NewsletterSubscriptionServiceProvider"
            ]
        }
    }
}
